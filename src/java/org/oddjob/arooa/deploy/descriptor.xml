<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<bean class="org.oddjob.arooa.deploy.ArooaDescriptorBean"
    namespace="http://rgordon.co.uk/oddjob/arooa" prefix="arooa">
	<values>
		<is element="descriptors" 
			className="org.oddjob.arooa.deploy.ListDescriptorBean"/>
		<is element="descriptor" 
			className="org.oddjob.arooa.deploy.ArooaDescriptorBean"/>
		<is element="bean-def" 
			className="org.oddjob.arooa.deploy.BeanDefinition"/>
        <is element="annotation" 
            className="org.oddjob.arooa.deploy.AnnotationDefinition"/>
		<is element="property"
			className="org.oddjob.arooa.deploy.PropertyDefinition"/>
		<is element="configuration"
			className="org.oddjob.arooa.types.XMLConfigurationType"/>
		<is element="magic-beans"
			className="org.oddjob.arooa.beanutils.MagicBeanDescriptorFactory"/>
		<is element="magic-bean"
			className="org.oddjob.arooa.beanutils.MagicBeanDefinition"/>
		<is element="magic-property"
			className="org.oddjob.arooa.beanutils.MagicBeanProperty"/>
	</values>
</bean>